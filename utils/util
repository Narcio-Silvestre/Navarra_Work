compare_quant = (a,b) =>{
    return b.quantidade - a.quantidade;
}

add_prev_consum = (data) => {
    data["previsao_consumo"]= data.quantidade * 5;
    return data;
}

filt_port = (a) =>{
    return a.pais == "PORT";
}

 
exports.prev_consum = add_prev_consum;
exports.compare_quant = compare_quant;
exports.filt_port = filt_port;